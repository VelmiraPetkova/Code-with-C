#include <stdio.h>

int yYear()
{
    int year;
    printf("Enter the Year:\t");
    scanf("%d", &year);
    int February_day;
    if(year%4 || year%400==0)
    {
       February_day=28;
    }
    else
    {
      February_day=29;  
    }

    return February_day;
}

int main()
{
    int d;
    printf("Enter number days:\t");
    scanf("%d", &d);

    switch(d)
    {
        case d<=31:
        printf("January\n");
        break;
        
        case d==31 + yYear():
       printf("february\n");
        break;
    }
    
    return 0;
    
}
