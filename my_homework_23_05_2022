//да се намери и отпечата най-горямото число от масив
#include <stdio.h>
#define SIZE 10
int main ()
{
    float A[SIZE];  //дефиниране на масив
    int i; //индекс на масива
    
//Инициализиране на масив    
      for(i=0;i<SIZE;i++)
      {
        printf("Enter num [%d]=",i);
        scanf("%f",&A[i]);
      }
    
   float max_val=A[0];
  
   for(i=0;i<SIZE;i++)
  {
      if(max_val<A[i])
      {
          max_val=A[i];
      }
  }

  printf("Max num is %f\n",max_val);
  return 0;
    
}

----------------------------------------
//Да се извадят нечетните елементи на масива
#include <stdio.h>
#define SIZE 10
int main()
{
    int A[SIZE];
    int i;
    
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d",&A[i]);
    }

  for(i=0;i<SIZE;i++)
  {
     if(A[i]%2!=0)
     {
       printf("%d\n",A[i]);  
     }
  }

 return 0;
    
}
