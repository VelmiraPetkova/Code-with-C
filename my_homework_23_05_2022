#include <stdio.h>
#define SIZE 8
#include <stdbool.h>
int main()

{
    int A[SIZE];
    int i;
    bool B=1;
//четем
   
   
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [ %d ]=",i);
        scanf("%d",&A[i]);
    }

    for(i=0 ;i<SIZE;i++)
    {
       
         if (A[i]!=A[SIZE-1-i])
         {
            //printf("Yes\n");
         }
         else 
         {
             printf("No\n");
         }
    }


/*извеждаме
    for(i=0;i<SIZE;i++)
    {
       printf("A[%d]=%d\n",i,A[i]);
    }    
        */
  return 0;  
}
