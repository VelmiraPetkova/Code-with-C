//Програма която проверява дали записаното число е огледално на последното
#include <stdio.h>
#define SIZE 8
#include <stdbool.h>
int main()

{
    int A[SIZE];
    int i;
    bool B=1;
//четем
   
   
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [ %d ]=",i);
        scanf("%d",&A[i]);
    }

    for(i=0 ;i<SIZE/2;i++)
    {
       
         if (A[i]!=A[SIZE-1-i])
         {
            B=0;
            //printf("Yes\n");
            break;
         }
   
    }
   
   if(B==1)
   {printf("YES\n");}
   else 
   {printf("NO\n");}


/*извеждаме
    for(i=0;i<SIZE;i++)
    {
       printf("A[%d]=%d\n",i,A[i]);
    }    
        */
  return 0;  
}
