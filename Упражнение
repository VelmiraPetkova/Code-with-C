//Задача 1 
//Да се напише програма, която въвежда от клавиатурата
//едномерен масив от 10 цели числа. 
//Програмата прочита едно цяло число x от клавиатурата
//И извежда колко пъти това число се среща в масива

#include <stdio.h>
#define Size 5
int main()
{
    int A[Size];
    int i;
    
    for(i=0;i<Size;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d", &A[i]);
    }
    int x;
    scanf("%d", &x);
    
    for(i=0;i<Size;i++)
    {
        
    }   
        if (x==A[i])
        {
            printf("%d\n", x);
        }
        else 
        {
            printf("The num is not found\n");
        }
        
        ----------------
        //Задача 1 
//Да се напише програма, която въвежда от клавиатурата
//едномерен масив от 10 цели числа. 
//Програмата прочита едно цяло число x от клавиатурата
//И извежда колко пъти това число се среща в масива

#include <stdio.h>
#define Size 5
int main()
{
    int A[Size];
    int i;
    
    for(i=0;i<Size;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d", &A[i]);
    }
    int x;
    scanf("%d", &x);
    int counter=0; 
    for(i=0;i<Size;i++)
    {
        
       if (x==A[i])
        {
            
           counter++;
        }
        
        
    }    
    
    
    printf("%d\n", counter);
    return 0;
}
    
    
    
    return 0;
}
