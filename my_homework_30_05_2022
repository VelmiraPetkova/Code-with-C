//Да се напише програма, която определя числата на кои позиции в масива
//са равни на квадрата на индекса си.
#include <stdio.h>
#define SIZE 10
int main()
{
    int A[SIZE];
    int i;
    
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d",&A[i]);
    }

    //int count=0;
    for(i=0;i<SIZE;i++)
    {
        if(A[i]==i*i)
        {
          //count++; 
          printf("%d\n",A[i]);
        }
    
       
    }
   
   return 0; 
    
}
