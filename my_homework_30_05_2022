// Даден е масив от10 символа. На кое място в масива се среща символът'A' 

#include <stdio.h>
int main()
{
    char A[10]={ '&','5','f','Y','6','?','0','2','A','T'};
    int i;
    
    //Четем масива
    
    
    for(i=0;i<10;i++)
    {
        if(A[i]==65)
        {
            printf("A[%d]=%c",i,A[i]);
        }
    }
return 0;
    
}

