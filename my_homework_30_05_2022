#include <stdio.h>
#define SIZE 10
int main()
{
    int A[SIZE];
    int i;
    
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d",&A[i]);
    }

    

//Колко пъти се среща числото
   int count[SIZE];
   int i_2;
  
   for(i=0;i<SIZE;i++)
   {
       count[i]=0;
       for(i_2=0;i_2<SIZE;i_2++)
       {
           if(A[i]==A[i_2])
           {count[i]++;}
           
       }
   }

 for(i=0;i<SIZE;i++)
 {
     printf("Number %d,was found %d times \n",A[i],count[i]);
 }
return 0;
    
}

//Да се напише програма, която въвежда от клавиатурата едномерен масив от 10 цели числа. Програмата прочита
//едно цяло число x от клавиатурата И извежда колко пъти това число се среща в масива
#include <stdio.h>
#define SIZE 10
int main()
{
    int A[SIZE];
    int i;
    
    for (i=0;i<SIZE;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d",&A[i]);
    }

   int num;
   scanf("%d",&num);
  
   int count=0;
   for(i=0;i<SIZE;i++)
   {
       if(A[i]==num)
       {
          count++;
       }
  
   }
   printf("Number %d,was found %d times \n",num,count);
   return 0; 
}
--------------------------------------
//Да се напише програма, която въвежда от клавиатурата едномерен масив от 10 целичисла. 
//Програмата определя колко от тях са отрицателни.
#include <stdio.h>
#define SIZE 5
int main()
{
    int A[SIZE];
    int i;
    for(i=0;i<SIZE;i++)
    {
        printf("Enter num [%d]=",i);
        scanf("%d",&A[i]);
    }
 
 int count=0; //смята броя повторения
 for(i=0;i<SIZE;i++)
 {
     if(A[i]<0)
     {
         count++;
     }
 }
 printf("Negative numbers are repeated %d time\n",count);
 return 0;
}
------------------------
//Програмата определя кои числа са цели числа и ги  извежда.
#include <stdio.h>
#define SIZE 10
int main()
{
    float A[SIZE]={1, 2.5 ,3 ,8 ,3.9, 47.5, 7,-8,9, -10.2 };
    int i;
    
    for(i=0;i<SIZE;i++)
    {
        if(A[i]==(int)A[i])
        {
            printf("%d\t", (int) A[i]);
        }
    }
    
}    
