//Да се напише програма, която определя в даден масив от 10 цели числа, колко от числата са двуцифрени.
#include <stdio.h>
#define SIZE 10

int main()
{
    float A[SIZE];
    int i;
    
    for(i=0;i<SIZE;i++)
    {
      printf("Enter num[%d]=",i);
      scanf("%f", &A[i]);
    }
    int count=0;
    for(i=0;i<SIZE;i++)
    {
      if(A[i]>9&&A[i]<100)
      {
          count++;
           
          
      }
       
        
    }
  printf("%d",count);
  return 0;  
}
